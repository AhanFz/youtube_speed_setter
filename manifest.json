{
    "manifest_version": 3,
    "name": "YouTube Speed Setter",
    "version": "1.0",
    "description": "Automatically sets YouTube video playback speed",
    "permissions": ["activeTab", "storage"],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["*://*.youtube.com/*"],
        "js": ["content.js"]
      }
    ]
  }
  